        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "laravel/laravel": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "6.21.1",
        "itsgoingd/clockwork": "^5.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bendt\\CoolLog\\Providers\\CoolLogServiceProvider"
            ]
        }
    }
}
