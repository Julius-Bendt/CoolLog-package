{
    "name": "bendt/coollog",
    "description": "Cool Log",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Julius Bendt",
            "email": "julius@juto.dk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bendt\\CoolLog\\": "src/"
        }
    },
    "autoload-dev": {
